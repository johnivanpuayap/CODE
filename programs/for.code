BEGIN CODE
    INT counter = 0, b = 2
    FOR (; counter < 10; counter = counter + b)
    BEGIN FOR
        DISPLAY: counter

        FOR(b = 0; b < 10; b = b + 1)
        BEGIN FOR
            DISPLAY: b & " <- b : " &counter
            CONTINUE
        END FOR

    END FOR

    DISPLAY: "Loop finished!"
END CODE